= render "partials/nav"

.msnry-wrap.msnry-projects
  .msnry.js-msnry
    - @projects.each do |project|
      .msnry-item{class: "msnry-item-#{project.size}"}
        .msnry-aspect-inner
          .msnry-item-bg.lazyload{'data-src': project.main_image_url}
          %noscript
            %img.msnry-logo{src: project.main_image_url}
          .msnry-item-title= project.title
          .msnry-item-dscr= project.subtitle

= render "partials/footer"
